    "collapsible_content_mGdRRb": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible_row_qdznRB": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Wer gestaltet die Tapeten?",
            "icon": "question_mark",
            "row_content": "<p>Alle Designs werden von Sindy, der Gründerin von Unik Nordic, persönlich entworfen. Jedes Motiv trägt ihre kreative Handschrift.</p>",
            "page": ""
          }
        },
        "collapsible_row_9LWUp6": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Wo werden die Produkte hergestellt?",
            "icon": "question_mark",
            "row_content": "<p>Unsere komplette Produktion findet in Berlin-Adlershof statt – klimafreundlich, ressourcenschonend und unter hohen Qualitätsstandards.</p>",
            "page": ""
          }
        },
        "collapsible_row_CXmCCG": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Sind die Produkte vegan & schadstofffrei?",
            "icon": "question_mark",
            "row_content": "<p>Ja. Unsere Produkte enthalten keine tierischen Bestandteile, sind frei von PVC, Weichmachern und Lösungsmitteln – und dünsten nicht aus.</p>",
            "page": ""
          }
        },
        "collapsible_row_kkXbFC": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Kann ich euch bei Unsicherheiten kontaktieren?",
            "icon": "question_mark",
            "row_content": "<p>Natürlich. Du erreichst uns per Mail oder über das Kontaktformular  und erhältst persönliche Unterstützung von Sindy und dem Unik Nordic-Team.</p>",
            "page": ""
          }
        }
      },
