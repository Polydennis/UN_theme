    "collapsible_content_eEQbk4": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible_row_zbHyHW": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Wie reinige ich die Folie?",
            "icon": "question_mark",
            "row_content": "<p>Du kannst die Folie mit einem feuchten Lappen oder Tuch und handelsüblichen Reinigern reinigen. Verwende aber bitte keine Scheuermilch. Durch die darin enthaltenen Partikel kann die Folie beschädigt werden.</p>",
            "page": ""
          }
        },
        "collapsible_row_RQRQVe": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Wie entferne ich die Folie?",
            "icon": "question_mark",
            "row_content": "<p>Die Folie kann rückstandslos abgezogen werden – am besten bei Raumtemperatur langsam und gleichmäßig.</p>",
            "page": ""
          }
        },
        "collapsible_row_LRKHHC": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Hinterlässt die Folie Rückstände?",
            "icon": "question_mark",
            "row_content": "<p>Nein. Auf glatten, geeigneten Untergründen bleibt nichts zurück.</p>",
            "page": ""
          }
        }
      },
